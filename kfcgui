

local function Notification(msg)
    game.StarterGui:SetCore("SendNotification", {
        Title = "Cash on the ground";
        Text = msg;
        Icon = "rbxassetid://0";
        Duration = 5;
    })
end

function comma_value(amount)
    local formatted = amount
    while true do  
      formatted, k = string.gsub(formatted, "^(-?%d+)(%d%d%d)", '%1,%2')
      if (k==0) then
        break
      end
    end
    return formatted
  end
